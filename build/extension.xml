<extension xmlns="http://ns.adobe.com/air/extension/3.1">
  <id>com.freshplanet.AirVideo</id>
  <versionNumber>1.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAirVideo.a</nativeLibrary>
                <initializer>AirVideoInitializer</initializer> 
                <finalizer>AirVideoFinalizer</finalizer>
            </applicationDeployment>
    </platform>
 
   <platform name="Android-ARM"> 
        <applicationDeployment> 
            <nativeLibrary>libAirVideo.jar</nativeLibrary> 
            <initializer>com.freshplanet.ane.AirVideo.Extension</initializer> 
            <finalizer>com.freshplanet.ane.AirVideo.Extension</finalizer> 
        </applicationDeployment>
    </platform>
    
    <platform name="default"> 
        <applicationDeployment/> 
    </platform> 
  </platforms>
</extension>